{
  "name": "rosalana/tracker",
  "description": "Rosalana tracker is a package that helps trace and log application activities in Rosalana ecosystem.",
  "type": "library",
  "keywords": [
    "laravel",
    "package",
    "rosalana",
    "tracker"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "michaelbany",
      "email": "mobany526@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0",
    "rosalana/core": "^0 || dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Rosalana\\Tracker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rosalana\\Tracker\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rosalana\\Tracker\\Providers\\RosalanaTracerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
